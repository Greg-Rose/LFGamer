<div class="row admin-consoles">
  <div class="col-xs-12">
    <%= link_to "#", class: "btn btn-lg admin-back-btn" do %>
      <span class="glyphicon glyphicon-remove-circle" aria-hidden="true"></span>
    <% end %>
  </div>

  <div class="col-xs-12 text-center">
    <%= link_to "Add Console", "#", class: "btn btn-primary", id: "add-console-btn" %>
  </div>

  <div class="col-xs-12 col-sm-10 col-sm-offset-1 consoles-table-div">
    <div class="table-responsive">
      <table class="table table-striped table-bordered">
        <tr>
          <th class="text-center">#</th>
          <th class="text-center">Name</th>
          <th class="text-center">Abbr.</th>
          <th class="text-center">Games</th>
          <th class="text-center">Created At</th>
        </tr>
        <%= render partial: "console", collection: @consoles %>
      </table>
    </div>
    <%= will_paginate @consoles, class: "text-center pagination-links" %>
  </div>
</div>
